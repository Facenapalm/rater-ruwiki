{
  "name": "rater",
  "version": "2.0.0",
  "description": "Wikipedia userscript that helps assess pages for WikiProjects",
  "homepage": "https://github.com/evad37/rater",
  "main": "rater-src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:es5": "jshint index.js index-sandbox.js",
    "lint:es6": "eslint index.js index-sandbox.js \"rater-src/**\"",
    "lint:fix": "eslint index.js index-sandbox.js \"rater-src/**\" --fix",
    "lint": "npm run lint:es6 && npm run lint:es5",
    "build:bundle": "browserify rater-src/App.js --debug -t babelify --outfile dist/rater.js",
    "build:minify": "uglifyjs dist/rater.js --compress --output dist/rater.min.js",
    "build": "npm run lint && npm run build:bundle && npm run build:minify"
  },
  "author": "",
  "license": "(MIT OR CC-BY-4.0)",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "eslint": "^6.5.1",
    "jshint": "^2.10.2",
    "uglify-js": "^3.6.3"
  }
}